#!/bin/sh
# postinst script for iotop
#
# see: dh_installdeb(1)

set -e

# generated by dh_installalternatives/13.1
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ] || [ "$1" = "abort-deconfigure" ] || [ "$1" = "abort-remove" ]; then
        update-alternatives --install /usr/sbin/iotop iotop /usr/sbin/iotop-py 50 \
            --slave /usr/share/man/man8/iotop.8.gz iotop.8.gz /usr/share/man/man8/iotop-py.8.gz
fi

#DEBHELPER#

exit 0
